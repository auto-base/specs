{
   "$schema": "https://json-schema.org/draft/2020-12/schema",
   "$id": "gitlab.spectrumdata.tech/ae/autobase/doc/api/available-calc",
   "$ref": "#/contentSchema/AvailableCalc",
   "contentSchema": {
      "AvailableCalc": {
         "properties": {
            "start_date": {
               "type": "string",
               "description": "StartDate - дата создания калькуляции"
            },
            "end_date": {
               "type": "string",
               "description": "EndDate - дата закрытия калькуляции"
            },
            "case_uid": {
               "type": "string",
               "description": "CaseUid - идентификатор дела"
            },
            "state": {
               "type": "string",
               "description": "State - статус калькуляции"
            },
            "calc_id": {
               "type": "integer",
               "description": "CalcId - идентификатор калькуляции"
            },
            "total_repair_price": {
               "type": "number",
               "description": "TotalRepairPrice - стоимость восстановительного ремонта"
            },
            "total_repair_price_with_run_out": {
               "type": "number",
               "description": "TotalRepairPriceWithRunOut - затраты на восстановительный ремонт(с учетом износа)"
            },
            "total_spare_price": {
               "type": "number",
               "description": "TotalSparePrice - общая стоимость всех деталей"
            },
            "total_spare_price_with_run_out": {
               "type": "number",
               "description": "TotalSparePriceWithRunOut - общая стоимость всех деталей с учетом износа"
            },
            "total_work_without_painting_cost": {
               "type": "number",
               "description": "TotalWorkWithoutPaintingCost - общая стоимость работ за исключением окрасочных работ"
            },
            "total_painting_cost": {
               "type": "number",
               "description": "TotalPaintingCost - общая стоимость окрасочных работ"
            },
            "total_materials_cost": {
               "type": "number",
               "description": "TotalMaterialsCost - общая стоимость материалов"
            }
         },
         "type": "object",
         "description": "AvailableCalc - доступная калькуляция"
      }
   }
}