{
   "$schema": "https://json-schema.org/draft/2020-12/schema",
   "$id": "gitlab.spectrumdata.tech/ae/autobase/doc/api/inspection-report-item",
   "$ref": "#/contentSchema/InspectionReportItem",
   "contentSchema": {
      "InspectionReportItem": {
         "properties": {
            "Num": {
               "type": "integer",
               "description": "Num - Порядковый номер записи в коллекции"
            },
            "Name": {
               "type": "string",
               "description": "Name - Наименование"
            },
            "NatureOfDamage": {
               "$ref": "../types/kernel_NatureOfDamage.json",
               "type": "string",
               "description": "NatureOfDamage - Характер повреждений"
            },
            "Replacement": {
               "type": "boolean",
               "description": "Replacement - Замена"
            },
            "Repair": {
               "type": "boolean",
               "description": "Repair - Ремонт"
            },
            "Coloring": {
               "type": "boolean",
               "description": "Coloring - Окраска"
            },
            "Diagnostics": {
               "type": "boolean",
               "description": "Diagnostics - Диагностика"
            },
            "RepairNorm": {
               "type": "number",
               "description": "RepairNorm - норматив ремонта"
            }
         },
         "type": "object",
         "description": "InspectionReportItem - строка таблицы Акта осмотра"
      }
   }
}